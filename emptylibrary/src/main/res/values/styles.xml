    <!-- 密码框的Edit样式-->
    <style name="Edit_Password_style">
        <item name="android:layout_height">@dimen/activity_title_kuan_40</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">@dimen/pading_kuan_5</item>
